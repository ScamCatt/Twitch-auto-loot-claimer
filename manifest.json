{
    "name": "Twitch auto-loot claimer",
    "description": "twitch extension",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.twitch.tv/*",
        "https://www.google.com"
    ]
}